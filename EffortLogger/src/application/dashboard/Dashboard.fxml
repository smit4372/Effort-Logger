<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="960.0" prefWidth="540.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.dashboard.DashboardController">
   <children>
      <ImageView fitHeight="261.0" fitWidth="210.0" layoutX="165.0" layoutY="281.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@user.png" />
      </ImageView>
      <Label layoutX="130.0" layoutY="133.0" text="Welcome, User!">
         <font>
            <Font size="42.0" />
         </font>
      </Label>
      <Button fx:id="newLogButton" layoutX="203.0" layoutY="600.0" mnemonicParsing="false" text="Add a Log">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="definitionsButton" layoutX="203.0" layoutY="670.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="135.0" text="Definitions">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
      <Button fx:id="pastLogsButton" layoutX="203.0" layoutY="740.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="135.0" text="History">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="logOutButton" layoutX="14.0" layoutY="911.0" mnemonicParsing="false" text="Log Out">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
